<!doctype html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
<link href='http://fonts.googleapis.com/css?family=Open+Sans+Condensed:300,700|Open+Sans:400italic,600italic,400,300,600,700' rel='stylesheet' type='text/css'>
  <link href='http://fonts.googleapis.com/css?family=Oswald:400,700,300' rel='stylesheet' type='text/css'>

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Surf & Paddle" %></title>

    <%= favicon_tag 'images/logo.png' %>

    <%= stylesheet_link_tag "all" %>
     <script src="//oss.maxcdn.com/jquery/2.1.1/jquery.js"></script>
    <%= javascript_include_tag  "all" %>
  </head>

  <body>

    <%= partial "layouts/hero" %>

    <%= yield %>

    <%= partial "layouts/footer" %>


  </body>
</html>
